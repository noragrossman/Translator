<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Oracle Corporation//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <servlet>
        <servlet-name>translate</servlet-name>
        <servlet-class>com.translator.TranslatorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>populate</servlet-name>
        <servlet-class>com.translator.PopulateDatastoreServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>conjugate</servlet-name>
        <servlet-class>com.translator.ConjugatorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mapreduce</servlet-name>
        <servlet-class>com.translator.MapReduceServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>translate</servlet-name>
        <url-pattern>/translate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>populate</servlet-name>
        <url-pattern>/populate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>conjugate</servlet-name>
        <url-pattern>/conjugate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mapreduce</servlet-name>
        <url-pattern>/mapreduce</url-pattern>
    </servlet-mapping>
    
    <!-- Added by Nora on April 11 -->
    <servlet>
      <servlet-name>mapreduce</servlet-name>
      <servlet-class>
        com.google.appengine.tools.mapreduce.MapReduceServlet
      </servlet-class>
    </servlet>
    <servlet-mapping>
      <servlet-name>mapreduce</servlet-name>
      <url-pattern>/mapreduce/*</url-pattern>
    </servlet-mapping>

    <servlet>
      <servlet-name>pipeline</servlet-name>
      <servlet-class>
        com.google.appengine.tools.pipeline.impl.servlets.PipelineServlet
      </servlet-class>
    </servlet>
    <servlet-mapping>
      <servlet-name>pipeline</servlet-name>
      <url-pattern>/_ah/pipeline/*</url-pattern>
    </servlet-mapping>
    <!-- end here -->
    
    <welcome-file-list>
        <welcome-file>translateHome.jsp</welcome-file>
    </welcome-file-list>
</web-app>